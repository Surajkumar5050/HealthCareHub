<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context=".HomeActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="128dp"
        android:layout_height="19dp"
        android:layout_marginTop="51dp"
        android:layout_marginBottom="821dp"
        android:background="@drawable/textview_border"
        android:text="HOME"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/buttonLabTest"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginStart="49dp"
        android:layout_marginTop="88dp"
        android:layout_marginEnd="57dp"
        android:background="@drawable/lab_test"
        app:layout_constraintBottom_toBottomOf="@+id/buttonBuyMedicine"
        app:layout_constraintEnd_toStartOf="@+id/buttonBuyMedicine"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
    />

    <ImageView
        android:id="@+id/buttonBuyMedicine"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginTop="88dp"
        android:layout_marginEnd="49dp"
        android:layout_marginBottom="62dp"
        android:background="@drawable/medicine_logo"
        app:layout_constraintBottom_toTopOf="@+id/buttonHealthArticles"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/buttonLabTest"
        app:layout_constraintTop_toBottomOf="@+id/textView"
         />

    <ImageView
        android:id="@+id/buttonFindDoctor"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginTop="62dp"
        android:layout_marginBottom="67dp"
        app:layout_constraintBottom_toTopOf="@+id/buttonOrderDetails"
        android:background="@drawable/doctor_logo"
        app:layout_constraintEnd_toEndOf="@+id/buttonLabTest"
        app:layout_constraintStart_toStartOf="@+id/buttonLabTest"
        app:layout_constraintTop_toBottomOf="@+id/buttonLabTest"
         />

    <ImageView
        android:id="@+id/buttonHealthArticles"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginBottom="67dp"
        app:layout_constraintBottom_toTopOf="@+id/buttonLogout"
        app:layout_constraintEnd_toEndOf="@+id/buttonBuyMedicine"
        app:layout_constraintStart_toStartOf="@+id/buttonBuyMedicine"
        app:layout_constraintTop_toBottomOf="@+id/buttonBuyMedicine"
        android:background="@drawable/health_articles"
         />

    <ImageView
        android:id="@+id/buttonOrderDetails"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginBottom="220dp"
        android:background="@drawable/order_logo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/buttonFindDoctor"
        app:layout_constraintStart_toStartOf="@+id/buttonFindDoctor"
        app:layout_constraintTop_toBottomOf="@+id/buttonFindDoctor"
         />

    <ImageView
        android:id="@+id/buttonLogout"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginBottom="220dp"
        android:background="@drawable/logout_logo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/buttonHealthArticles"
        app:layout_constraintStart_toStartOf="@+id/buttonHealthArticles"
        app:layout_constraintTop_toBottomOf="@+id/buttonHealthArticles"
         />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Lab Test"
        app:layout_constraintBottom_toBottomOf="@+id/textView5"
        app:layout_constraintEnd_toEndOf="@+id/buttonLabTest"
        app:layout_constraintStart_toStartOf="@+id/buttonLabTest"
        app:layout_constraintTop_toBottomOf="@+id/buttonLabTest" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="32dp"
        android:text="Buy Medicine"
        app:layout_constraintBottom_toTopOf="@+id/buttonHealthArticles"
        app:layout_constraintEnd_toEndOf="@+id/buttonBuyMedicine"
        app:layout_constraintStart_toStartOf="@+id/buttonBuyMedicine"
        app:layout_constraintTop_toBottomOf="@+id/buttonBuyMedicine" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Find Doctor"
        app:layout_constraintBottom_toBottomOf="@+id/textView7"
        app:layout_constraintEnd_toEndOf="@+id/buttonFindDoctor"
        app:layout_constraintStart_toStartOf="@+id/buttonFindDoctor"
        app:layout_constraintTop_toBottomOf="@+id/buttonFindDoctor" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="41dp"
        android:text="Health Articles"
        app:layout_constraintBottom_toTopOf="@+id/buttonLogout"
        app:layout_constraintEnd_toEndOf="@+id/buttonHealthArticles"
        app:layout_constraintStart_toStartOf="@+id/buttonHealthArticles"
        app:layout_constraintTop_toBottomOf="@+id/buttonHealthArticles" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Order Details"
        app:layout_constraintBottom_toBottomOf="@+id/textView9"
        app:layout_constraintEnd_toEndOf="@+id/buttonOrderDetails"
        app:layout_constraintStart_toStartOf="@+id/buttonOrderDetails"
        app:layout_constraintTop_toBottomOf="@+id/buttonOrderDetails" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="193dp"
        android:text="Logout"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/buttonLogout"
        app:layout_constraintStart_toStartOf="@+id/buttonLogout"
        app:layout_constraintTop_toBottomOf="@+id/buttonLogout" />
</androidx.constraintlayout.widget.ConstraintLayout>